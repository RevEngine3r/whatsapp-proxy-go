# WhatsApp Proxy Go - Docker Environment Variables

# Server Configuration
PROXY_PORT=8443
PROXY_BIND_ADDR=0.0.0.0
PROXY_IDLE_TIMEOUT=300
PROXY_MAX_CONNECTIONS=1000

# SOCKS5 Upstream Proxy
SOCKS5_ENABLED=false
SOCKS5_HOST=127.0.0.1
SOCKS5_PORT=1080
SOCKS5_USERNAME=
SOCKS5_PASSWORD=

# SSL/TLS
SSL_AUTO_GENERATE=true

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
LOG_OUTPUT=stdout

# Metrics
METRICS_ENABLED=true
METRICS_PORT=8199
METRICS_BIND_ADDR=0.0.0.0

# Docker Build
VERSION=latest
BUILD_TIME=
